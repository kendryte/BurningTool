{
	"[jsonc]": {
		"editor.defaultFormatter": "vscode.json-language-features"
	},
	"[json]": {
		"editor.defaultFormatter": "vscode.json-language-features"
	},
	"cmake.configureOnEdit": false,
	"cmake.saveBeforeBuild": true,
	"cmake.buildBeforeRun": true,
	"cmake.clearOutputBeforeBuild": true,
	"cmake.sourceDirectory": "${workspaceFolder}",
	"cmake.buildDirectory": "${workspaceFolder}/build-vscode-${env:HOSTNAME}${env:COMPUTERNAME}",
	"cmake.copyCompileCommands": "${workspaceRoot}/.vscode/compile_command.json",
	"cmake.exportCompileCommandsFile": true,
	"cmake.skipConfigureIfCachePresent": true,
	"cmake.cmakeCommunicationMode": "fileApi",
	"cmakeLanguageSupport.enableFileAPI": true,
	"cmake.enabledOutputParsers": [
		"cmake",
		"gcc",
		"gnuld",
	],
	"cmake.configureSettings": {
		"CMAKE_TOOLCHAIN_FILE": "${workspaceFolder}/vcpkg/scripts/buildsystems/vcpkg.cmake",
		"DISABLE_TERM_HYPERLINK": "yes",
		"OpenGL_GL_PREFERENCE": "GLVND"
	},
	"git.autoRepositoryDetection": false,
	"search.exclude": {
		"**/vcpkg": true,
		"**/build-*": true
	},
	"files.associations": {
		"*.CMakeLists": "cmake",
		"qcheckbox": "cpp",
